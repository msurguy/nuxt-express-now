{
  "name": "nuxt-express-now",
  "version": "1.0.11",
  "description": "Demo project for Nuxt / Express deployed on Now via GH Actions",
  "author": "Maks Surguy",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "deploy-prod": "now -A prod.now.json && now alias -A prod.now.json",
    "deploy-staging": "now -A staging.now.json && now alias -A staging.now.json",
    "deploy-dev": "now -A dev.now.json && now alias -A dev.now.json",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/style-resources": "^1.0.0",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-basic-auth": "^1.2.0",
    "nuxt": "^2.9.2",
    "nuxt-basic-auth-module": "^1.2.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "sass-loader": "^7.1.0"
  },
  "engines": {
    "node": ">=10.x"
  }
}
